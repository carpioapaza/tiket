<div class="container">
  <h1 class="mt-4">My Events</h1>
  <table class="table">
    <thead>
      <tr>
        <th>Event Name</th>
        <th>Status</th>
        <th>Actions</th>
      </tr>
    </thead>
    <tbody>
      <% @events.each do |event| %>
        <tr>
          <td><%= event.name %></td>
          <td><%= event.admin_status.titleize %></td>
          <td>
            <%= link_to 'Show', admins_event_path(event), class: "btn btn-info" %>
            <%= link_to 'Edit', edit_admins_event_path(event), class: "btn btn-warning" %>
          </td>
        </tr>
      <% end %>
    </tbody>
  </table>
</div>
